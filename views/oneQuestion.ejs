<%- include('./components/header')%>
<div class="container">
    <% if (result) { %>
        <div class="card">
            <div class="card-body">
              <h5 class="card-title"><%= result.title %></h5>
              <p class="card-text"><%= result.message %></p>
              <a href=/question/edit/<%= result._id%> class="btn btn-outline-dark">Edit</a>
              <a href=/question/delete/<%= result._id%> class="btn btn-outline-dark">Delete</a>
              <a href="/" class="btn btn-outline-dark">Go home</a>
            </div>
          </div>
        <% } else { %>
                there is no record
    <% } %>
 </div>

<%- include('./components/footer')%>