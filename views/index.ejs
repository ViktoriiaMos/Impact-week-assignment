    </div>   
</div>
<div class="add">
    <a href="/addQuestion" class="btn btn-outline-dark">Add question</a>
</div> <br>
<% if (result.length> 0 ) {%>
    <% result.forEach(element=> { %>
  <div class="card  add-question">
